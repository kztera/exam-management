{
  "name": "simple-vite-react-express",
  "version": "1.0.0",
  "description": "Boilerplate to build fullstack app using react, express, vite and pg",
  "main": "src/server/index.ts",
  "type": "module",
  "scripts": {
    "build": "npx vite build",
    "start": "npm run build && node src/server/index.ts",
    "server": "nodemon src/server/index.ts",
    "client": "npx vite",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "migrate": "npx prisma migrate dev"
  },
  "author": "Avi Maiti",
  "license": "ISC",
  "dependencies": {
    "@mantine/core": "^7.15.1",
    "@mantine/dates": "^7.15.1",
    "@mantine/dropzone": "^7.15.1",
    "@mantine/form": "^7.15.1",
    "@mantine/hooks": "^7.15.1",
    "@mantine/notifications": "^7.15.1",
    "@prisma/client": "^6.2.1",
    "@tabler/icons-react": "^3.32.0",
    "concurrently": "^9.1.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "express-validator": "^7.2.1",
    "fs-extra": "^11.2.0",
    "helmet": "^8.0.0",
    "ky": "^1.7.2",
    "lodash": "^4.17.21",
    "multer": "^1.4.5-lts.1",
    "prisma": "^6.2.1",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.1",
    "strong-error-handler": "^5.0.15",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.3",
    "@types/lodash": "^4.17.13",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.5",
    "@types/react": "^19.0.2",
    "@types/react-dom": "^19.0.2",
    "@vitejs/plugin-react": "^4.3.4",
    "nodemon": "^3.1.9",
    "tsx": "^4.20.3",
    "typescript": "^5.7.2",
    "vite": "^6.0.7"
  }
}
